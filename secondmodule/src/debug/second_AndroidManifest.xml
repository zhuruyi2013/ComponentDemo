<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          xmlns:tools="http://schemas.android.com/tools"
          package="com.zry.secondmodule">


    <application
            android:name=".SecondDebugApplication"
            android:allowBackup="true"
            android:icon="@mipmap/plane"
            android:label="secondmodule"
            android:supportsRtl="true"
            tools:replace="android:label">

        <activity android:name=".SecondDebugActivity"
                  android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.DEFAULT"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity
                android:name=".SecondActivity"
                android:label="@string/secondmodule_title_activity_second"
                android:theme="@style/AppTheme.NoActionBar"/>

    </application>

</manifest>
